<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Velirano!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <!-- Bootstrap core CSS -->
        <link href="{{asset('bundles/app/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet">
        <link href="{{asset('bundles/app/offcanvas/css/offcanvas.css')}}" rel="stylesheet">
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script src="{{asset('bundles/app/jquery/jquery.min.js')}}"></script>
        <script src="{{asset('bundles/app/bootstrap/js/bootstrap.bundle.min.js')}}"></script>
        <script src="{{asset('bundles/app/count_down.js')}}"></script>
        {% endblock %}
    </body>
</html>
